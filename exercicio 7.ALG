Algoritmo "semnome"

Var
n1,n2,dividendo,divisor:inteiro
Quociente,resto:real
Inicio
escreval ("Escreva um numero")
leia (n1)
escreval ("Escreva outro numero")
leia (n2)
dividendo <-(n1)
divisor <- (n2)
quociente <- (n1/n2)
resto <- (n1%n2)
escreval ("divisor ", divisor,", dividendo ", dividendo, ", quociente ", quociente,", resto ", resto)
Fimalgoritmo
