algoritmo "semnome"

var
 numero1, numero2, resultado:real

inicio
escreval("digite um numero")
leia (numero1)
escreval("digite outro numero")
leia (numero2)
se (numero1 > numero2) entao
resultado <- numero1
senao
resultado <- numero2
escreval("o valor",resultado)

fimse
fimalgoritmo
